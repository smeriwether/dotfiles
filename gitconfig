[core]
  excludesfile = ~/.gitignore
  editor = vim
	attributesfile = /Users/srm/.gitattributes

[alias]
  aa = add --all
  amend = commit --amend
  b = !git --no-pager branch -v
  ci = commit
  dc = diff --cached
  delete = branch -D
  st = status
  undo = reset --soft HEAD^

[color]
  ui = auto

[branch "master"]
  remote = origin
  merge = refs/heads/master
  rebase = true
