[user]
  name = Stephen Meriwether
  email = smeriwether@hey.com

[core]
  excludesfile = ~/.gitignore
  editor = vim
	attributesfile = /Users/srm/.gitattributes

[alias]
  aa = add --all
  amend = commit --amend
  b = !git --no-pager branch -v
  ci = commit
  co = checkout
  dc = diff --cached
  delete = branch -D
  pod = pull origin develop
  pom = pull origin main
  st = status
  undo = reset --soft HEAD^

[color]
  ui = auto

[branch "master"]
    remote = origin
    merge = refs/heads/master
    rebase = true
